{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "typescript.tsx",
    "javascript.jsx"
  ],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "typescript.tsx",
    "javascript.jsx"
  ],
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus" : true,
  "explorer.quitOnOpen": true,
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "coc.preferences.colorSupport": true,
  "highlight.colors.enable": true,
  "cSpellExt.enableDictionaries": ["spanish"],
  "cSpell.language": "en,es",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "cSpell.diagnosticLevel": "Hint",
  "git.enableGutters": true,
  "diagnostic.warningSign": "âš ",
  "diagnostic.hintSign": "!",
  "prettier.eslintIntegration": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "javascript.js",
    "typescript.ts",
    "json",
    "html",
    "css"
  ],
  "session.saveOnVimLeave": false,
  "session.directory": "~/.config/nvim/sessions/",
  "cSpell.userWords": ["Lonighi", "devtool"],
  "snippets.textmateSnippetsRoots":[ "~/.config/nvim/snippets" ]
}
